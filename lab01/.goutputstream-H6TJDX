#include <iostream>

int series_program(int number){
    int i = 1;
    int j = 2;
    int product = 0;
    while (number > product){
        std::cout << i << " ";
        product = i * j;
        i = j;
        j = product;
    }
    std::cout << j;
}

int main(){
    int A;
    
    std::cout << "Enter a postive integer: ";
    std::cin >> A;
    std::cout << std::endl;
    
    series_program(A);
    
    return 0;
}
