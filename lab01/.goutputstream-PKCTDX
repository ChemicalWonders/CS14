#include <iostream>

int series_program(int number){
    int i = 1;
    int j = 2;
    int product = 0;
    while (number > product){
        std::cout << i;
        product = i * j;
        i = j;
        j = product;
    }
}

int main(){
    int A;
    
    cout << "Enter a postive integer: ";
    cin >> A;
    cout << endl;
    
    series_program(A);
    
    return 0;
}
