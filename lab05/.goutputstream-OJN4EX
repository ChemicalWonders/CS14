int my_func(struct node* node)
{
    if (node == NULL)
        return 1;
    
    if (node-> left != NULL && node->left->data > node->data)
        return 0;
    
    if (node->right != NULL && node->right->data <= node->data)
        return 0;
        
    if(!my_func(node->left) || !my_func(node->right))
        return 0;
        
    return 1;
}
//
