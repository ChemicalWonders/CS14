//Lab 3: Problem #3
#include <iostream>
#include <list>

using namespace std;

class Node{
    private:
        char value;
        Node* next;
        
    public:
        Node():value(), next(NULL){};
        Node(char val): value(val), next(NULL){};
    };
    
void reversal(Node* pointer){
    if (pointer == NULL){
        return;
    }
    list<int> reversed_list;
    
    reversed_list.push_back(reversal(pointer->next));
}


int main(){
    list<int> alpha;
    
    alpha.push_back(1);
    alpha.push_back(2);
    alpha.push_back(3);
    
    for (list<int>::iterator j = alpha.begin(); j != alpha.end(); ++j)
        cout << ' ' << *j;
    cout << endl;

    return 0;
}
