#include <iostream>
#include <list>
using namespace std;

int main(){
    list <int> A;
    int n = 0;
    int b = 0;
    
    A.push_back(1);
    A.push_back(2);
    A.push_back(3);
    A.push_back(4);
    
    cout << "Which node would you like?" << endl;
    cin >> n;
    
    cout << "The list is: ";
    cout << endl;
    
    for (list<int>::iterator k = A.begin(); k != A.end(); ++k)
        cout << ' ' << *k;
    cout << endl;
    
    cout << "The number is:";
    b = A.size();
    for (list<int>::iterator j = A.end(); j != A.begin(); --j){
        
        if ((n-b) == n){
            cout << " " << *j;
        }
        --b;
    }
    cout << endl;

    return 0;
}
