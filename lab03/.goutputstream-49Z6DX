// Problem # 5: Determine if a singly linked list is circular or not
#include <iostream>
#include <list>
using namespace std;

int main(){
    list<int> A;
    A.push_back(1);
    A.push_back(2);
    A.push_back(3);
    //A.end() = A.begin();
    int tick = 0;
    int Asize = A.size();
    
    for (list<int>::iterator j = A.begin(); j != A.end(); ++j){
        for (list<int>::iterator k = a.begin(); k != a.end(); ++k){
            if (j == k){
                cout << "The list is circular, because tail is pointing to head";
                cout << endl;
                ++tick;
            }
        }
    }
    
    if (tick == 0){
        cout << "Congrats! Your list is not circular";
    }
    
    return 0;
}
    
    
