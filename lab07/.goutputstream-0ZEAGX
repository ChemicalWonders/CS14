#include <iostream>
#include <cstdlib>

//Lab # 7

//Name: Kevin Chan
//Lab: 8AM to 11AM

using namespace std;

//Problem #1
void arrange ( int * array, int size ){
    int isFalse = 0;
    int isMaybe = 0;
    int isTrue = 0;
    
    for (int i = 0; i < size; ++i){
        if (array[i] == 1){
            ++isTrue;
        }
        else if (array [i] == 0){
            ++isFalse;
        }
        
        else if (array[i] == 2) {
            ++isMaybe;
        }
    }
    
    for (int j = 0; j < isFalse; ++j) {
        array[j] = 0;
    }
    
    for (int k = isFalse; k < isMaybe+isFalse; ++k) {
        array[k] = 2;
    }
    
    for (int l = isFalse + isMaybe; l < size; ++l){
        array[l] = 1;
    }
    
}
int main(){
    
    return 0;
}
