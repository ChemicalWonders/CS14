// Problem # 6: Computing L1 U L2 using only basic list operations

#include <iostream>
#include <list>
#include <vector>

using namespace std;

void printLots (list<int> L, list<int> P)
{
    P.sort(); // Sorts the list from smallest to biggest.
    P.unique(); //Removes any duplicates; Linear time.
    
    vector<int> printOut;
    
    for (list<int>::iterator j = P.begin(); j != P.end(); ++j){
        printOut.push_back(*j);
        cout << ' ' << *j; // To test if it is actually pushing back;
    } // This portion is linear.
    
    int counter = 0;    
    int x = 0;
    
    for (list<int>::iterator i = L.begin(); i != L.end(); ++i){
        if (printOut.at(x) == counter){
            cout << ' ' << *i;
        }
        x++;
        counter++;
    }
    cout << '\n';
    return;
}


int main()
{
    list <int> alpha, beta;
    alpha.push_back(1);
    alpha.push_back(2);
    alpha.push_back(3);
    
    beta.push_back(1);
    
    printLots(alpha, beta);
    
    return 0;
}
